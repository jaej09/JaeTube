extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 Searching for: #{searchingBy} 
    .search__videos
        each video in videos
            +videoBlock({
                videoFile: video.videoFile,
                poster: video.poster,
                title: video.title,
                views: video.views,
                description: video.description,
            })
